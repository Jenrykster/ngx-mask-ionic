{
  "name": "ngx-mask-ionic",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:build": "ng build ngx-mask-ionic-lib",
    "lib:build-watch": "ng build ngx-mask-ionic-lib --watch",
    "lib:copy-readme": "node_modules/.bin/copyfiles ./README.md ./dist/ngx-mask-ionic-lib",
    "lib:publish": "npm run lib:build && npm run lib:copy-readme && cd dist/ngx-mask-ionic-lib && npm publish",
    "lint-fix": "node_modules/.bin/tslint --fix --project tsconfig.json",
    "format-ts": "node_modules/.bin/prettier --write --list-different \"src/**/*.ts\"",
    "format-html": "node_modules/.bin/glob-run html-beautify -r \"src/**/*.html\"",
    "format-css": "node_modules/.bin/glob-run css-beautify -r \"src/**/*.scss\"",
    "lib:format-ts": "node_modules/.bin/prettier --write --list-different \"projects/**/*.ts\"",
    "lib:format-html": "node_modules/.bin/glob-run html-beautify -r \"projects/**/*.html\"",
    "lib:format-css": "node_modules/.bin/glob-run css-beautify -r \"projects/**/*.scss\"",
    "lint-format": "npm run lint-fix && npm run format-ts && npm run format-html && npm run format-css && npm run lib:format-ts && npm run lib:format-html && npm run lib:format-css",
    "check-for-tslint-prettier-conflicts": "node_modules/.bin/tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/http": "~7.2.16",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "core-js": "^3.22.3",
    "node-fetch": "3",
    "rxjs": "~7.5.5",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@angular/language-service": "~13.0.0",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~17.0.2",
    "codelyzer": "~6.0.2",
    "copyfiles": "^2.4.1",
    "glob-run": "^0.1.7",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "js-beautify": "^1.14.3",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.0.0",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.7.0",
    "tsickle": ">=0.46.0",
    "tslib": "^2.4.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.4.2"
  }
}
